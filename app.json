{
  "expo": {
    "name": "Movies-ReactNative",
    "slug": "Movies-ReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favEmpty.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/favEmpty.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
       "bundleIdentifier": "com.movies.reactNative"
    },
    "android": {
       "package": "com.movies.reactNative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
